<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>completion_testServlet</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>com.completion_test.web.servlet.completion_testServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>completion</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>com.completion_test.web.servlet.completions</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>completion_testServlet</servlet-name>
    <url-pattern>/servlet/completion_testServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>completion</servlet-name>
    <url-pattern>/servlet/completion</url-pattern>
  </servlet-mapping>

</web-app>